        "parameters" : [ {
          "name" : "count",
          "description" : "The number of facts to return",
          "in" : "query",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          },
          "example" : "3"
        }, {
          "name" : "lang",
          "in" : "query",
          "required" : false,
          "style" : "form",
          "description" : "The language to return the facts",
          "explode" : true,
          "schema" : {
            "type" : "string",
            "default" : "eng",
            "enum": ["eng","ger","ukr","rus","por","esp","cze","fil","urd"]
          },
          "example" : "eng"
        } ],
        "responses" : {
          "200" : {
            "description" : "A list of facts about cats",
            "content" : {
              "application/json; charset=utf-8" : {
                "schema" : { },
                "examples" : { }
              }
            }
          }
        }
      }
    }
  },
  "components" : { }
}